<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="@color/white300" />
        </shape>
    </item>
    <item>
        <rotate
            android:fromDegrees="-45">
            <shape android:shape="line">
                <stroke
                    android:width="2dp"
                    android:color="@color/white300" />
            </shape>
        </rotate>
    </item>
</layer-list>
